  "dependencies": {
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "ws": "^7.0.1"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "jsdoc": "^3.6.2",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "prettier": "^1.18.2"
