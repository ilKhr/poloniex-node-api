  "dependencies": {
    "@types/node": "^12.7.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.2",
